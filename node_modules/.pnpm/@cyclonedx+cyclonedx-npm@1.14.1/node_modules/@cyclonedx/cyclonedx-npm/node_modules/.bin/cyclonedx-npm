#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hermes/Projetos/nodecli/pegaFPM/node_modules/.pnpm/@cyclonedx+cyclonedx-npm@1.14.1/node_modules/@cyclonedx/cyclonedx-npm/bin/node_modules:/home/hermes/Projetos/nodecli/pegaFPM/node_modules/.pnpm/@cyclonedx+cyclonedx-npm@1.14.1/node_modules/@cyclonedx/cyclonedx-npm/node_modules:/home/hermes/Projetos/nodecli/pegaFPM/node_modules/.pnpm/@cyclonedx+cyclonedx-npm@1.14.1/node_modules/@cyclonedx/node_modules:/home/hermes/Projetos/nodecli/pegaFPM/node_modules/.pnpm/@cyclonedx+cyclonedx-npm@1.14.1/node_modules:/home/hermes/Projetos/nodecli/pegaFPM/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/hermes/Projetos/nodecli/pegaFPM/node_modules/.pnpm/@cyclonedx+cyclonedx-npm@1.14.1/node_modules/@cyclonedx/cyclonedx-npm/bin/node_modules:/home/hermes/Projetos/nodecli/pegaFPM/node_modules/.pnpm/@cyclonedx+cyclonedx-npm@1.14.1/node_modules/@cyclonedx/cyclonedx-npm/node_modules:/home/hermes/Projetos/nodecli/pegaFPM/node_modules/.pnpm/@cyclonedx+cyclonedx-npm@1.14.1/node_modules/@cyclonedx/node_modules:/home/hermes/Projetos/nodecli/pegaFPM/node_modules/.pnpm/@cyclonedx+cyclonedx-npm@1.14.1/node_modules:/home/hermes/Projetos/nodecli/pegaFPM/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cyclonedx-npm-cli.js" "$@"
else
  exec node  "$basedir/../../bin/cyclonedx-npm-cli.js" "$@"
fi
